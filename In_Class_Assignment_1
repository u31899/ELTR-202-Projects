#include <P18F4620.inc>

BuildStart:
	
file30 	set 0x30
file20	set	0x20			;set labels
file50	set 0x50

	bra StepOne

StepTwo:
	movlw	0x2a			;add 2a to wreg
	movwf	file30, A		;add 2a to gpr 20
	bra InfinateLoopInt

StepOne:
	movlw	0x1c			;add 1c to wreg
	movwf	file20, A		;move wreg value to gpr 20
	bra StepTwo

InfinateLoopInt
	clrf	file50
	movlw 	0x01			;Add 01 to wreg
	movwf	file50, A			;add 01 to gpr 0x50
InfinateLoop
	movlw	0x01
	addwf	file50, A		
	movwf	file50, A
	bra InfinateLoop

ProgramEnd:

	nop
	end
